@model Travelista.ViewModel.TripViewModel


<html lang="zxx">
<head>
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<link rel="shortcut icon" href="img/fav.png">
	<!-- Author Meta -->
	<meta name="author" content="colorlib">
	<!-- Meta Description -->
	<meta name="description" content="">
	<!-- Meta Keyword -->
	<meta name="keywords" content="">
	<!-- meta character set -->
	<meta charset="UTF-8">
</head>

<body>
	<!--Section Page Title-->
	<section class="about-banner relative">
		<div class="overlay overlay-bg"></div>
		<div class="container">
			<div class="row d-flex align-items-center justify-content-center">
				<div class="about-content col-lg-12">
					<h1 class="text-white">
						Tour Packages
					</h1>
					<p class="text-white link-nav"><a asp-action="Index" asp-controller="Home">Home </a>  <span class="lnr lnr-arrow-right"></span>  <a asp-action="Index" asp-controller="Trips"> Trips</a></p>
				</div>
			</div>
		</div>
	</section>

	<!-- Start Trips Area -->
	<section class="destinations-area section-gap">
		    @Html.Partial("_FilterPartial", Model)
		<div class="row">
			@if (Model.trips.Any())
			{
				@Html.Partial("_TripsPartial", Model)
			}
			else
			{
				<div class="col-lg-12 text-center">
					<br>
					<h1 class="display-4">Not Exist Trips</h1>
				</div>
			}
			</div>
			<div id="pagination" class="mt-3">

			</div>
		</div>
	</section>

	
</body>
</html>





